  title = {{{SAT-based}} Counterexample-Guided Inductive Synthesis of Distributed Controllers},
  date = {2020},
  journaltitle = {IEEE Access},
  volume = {8},
  pages = {207485--207498},
  issn = {2169-3536},
  doi = {10.1109/ACCESS.2020.3037780},
  abstract = {This paper proposes a new method for automatic synthesis of distributed discrete-state controllers from given temporal specification and behavior examples. The proposed method develops known synthesis methods to the distributed case, which is a fundamental extension. This method can be applied for automatic generation of correct-by-design distributed control software for industrial automation. The proposed approach is based on reduction to the Boolean satisfiability problem (SAT) and has Counterexample-Guided Inductive Synthesis (CEGIS) at its core. We evaluate the proposed approach using the classical distributed alternating bit protocol.},
  langid = {english},
}

